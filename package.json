{
  "name": "@galligan/logbooks-workspace",
  "version": "0.0.1",
  "private": true,
  "description": "Logbooks field recording service workspace",
  "type": "module",
  "scripts": {
    "build": "pnpm -r build && tsx scripts/add-generated-headers.ts",
    "build:packages": "pnpm -r build",
    "test": "pnpm -r test",
    "test:coverage": "pnpm -r test:coverage",
    "test:watch": "pnpm -r test:watch",
    "test:all": "vitest run",
    "test:all:coverage": "vitest run --coverage",
    "test:all:watch": "vitest",
    "test:integration": "pnpm -r test:integration",
    "dev": "pnpm -r dev",
    "clean": "pnpm -r clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check:all": "pnpm run lint && pnpm run format:check",
    "typecheck": "tsc --noEmit",
    "ci:local": "pnpm run check:all && pnpm run typecheck && pnpm run test"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@faker-js/faker": "^9.8.0",
    "@types/node": "^22.15.30",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitest/coverage-v8": "^3.2.2",
    "better-sqlite3": "^11.10.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.2"
  },
  "packageManager": "pnpm@10.11.1"
}
